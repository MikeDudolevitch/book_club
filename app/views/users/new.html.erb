<h1> Sign-Up</h1>
<%= form_with(model: @user, url: signup_path) do |f| %>
    <%= f.label "email" %>
    <%= f.text_field "email" %>
    <%= f.label "username" %>
    <%= f.text_field "username" %>
    <%= f.label "password" %>
    <%= f.password_field "password" %>
    <%= f.submit "Let's Party" %>
    <% end %>